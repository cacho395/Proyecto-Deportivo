@page
@model Frontend.Pages.CPatrocinador.DeleteModel
@{
    ViewData["Title"]="Eliminar Patrocinador";
}

<h2>Eliminar Patrocinador</h2>
@if(ViewData["Error"]==null)
{
    <h3 class="text-danger">Esta seguro de eliminar</h3>
}
else
{
    <h3 class="text-danger">@ViewData["Error"]</h3>
}



<div class="form-horizontal">
    <hr/>
    <div class="row">
        <div class="col-md-4">
            <form method="post">
                <input type="hidden" asp-for="Patrocinador.Id"/>
                <div class="form-group">
                    <label asp-for="Patrocinador.Documento" class="control-label"></label>
                    <input asp-for="Patrocinador.Documento" class="form-control" readonly/>
                    
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.Nombre" class="control-label"></label>
                    <input asp-for="Patrocinador.Nombre" class="form-control" readonly/>
                    
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.Tipo" class="control-label"></label>
                    <input asp-for="Patrocinador.Tipo" class="form-control" readonly/>
                    
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.Direccion" class="control-label"></label>
                    <input asp-for="Patrocinador.Direccion" class="form-control" readonly/>
                   
                </div>

                <div class="form-group">
                    <label asp-for="Patrocinador.Telefono" class="control-label"></label>
                    <input asp-for="Patrocinador.Telefono" class="form-control" readonly/>
                   
                </div>
                <br>
                <div class="form-group">
                    <input type="submit" value="Eliminar" class="btn btn-outline-danger"/>
                    <a asp-page="Index" class="btn btn-outline-secondary">Cancelar</a>
                </div>

            </form>
        </div>
    </div>
</div>
